{
    "TOKEN_SALUTATION": {
        "de": {
            "pattern": "(Herrn?|Frau|Fr\\.)",
            "flags": "i",
            "description": "Deutsche Anreden (geschlechtsbezogene Anredeformen)",
            "examples": [
                "Herr",
                "Herrn",
                "Frau",
                "Fr."
            ],
            "category": "salutation"
        },
        "en": {
            "pattern": "(Mr\\.?|Mrs\\.?|Ms\\.?|Miss|Mx\\.?)",
            "flags": "i",
            "description": "Englische Anreden (geschlechtsbezogene oder neutrale Anredeformen)",
            "examples": [
                "Mr.",
                "Mrs",
                "Ms.",
                "Miss",
                "Mx."
            ],
            "category": "salutation"
        },
        "fr": {
            "pattern": "(Monsieur|Madame|Mademoiselle|M\\.|Mme|Mlle)",
            "flags": "i",
            "description": "Französische Anreden",
            "examples": [
                "Monsieur",
                "Madame",
                "Mlle",
                "M.",
                "Mme"
            ],
            "category": "salutation"
        }
    },
    "TOKEN_TITLE": {
