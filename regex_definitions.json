    "TOKEN_PARTICLE": {
        "de": {
            "pattern": "(von(?: der| dem| den)?|zu(?:r|m)?|auf|aus|am|an|im|in)",
            "flags": "i",
            "description": "Deutsche Adelsprädikate und Namenspartikel",
            "examples": [
                "von",
                "von der",
                "zu",
                "von dem"
            ],
            "category": "particle"
        },
        "en": {
            "pattern": "(van(?: der)?|von|de(?: la)?|du|da|del|della|di|dos|st\\.|st)",
            "flags": "i",
            "description": "Häufige Namenspartikel in englischsprachigen Kontexten (oft aus anderen Sprachen übernommen)",
            "examples": [
                "van",
                "van der",
                "de",
                "del",
                "da",
                "St.",
                "st"
            ],
            "category": "particle"
        },
        "fr": {
            "pattern": "(de(?: la)?|du|des|d['’]|l['’]|le|la)",
            "flags": "i",
            "description": "Französische Partikel und Artikel in Namen",
            "examples": [
                "de",
                "de la",
                "du",
                "d'",
                "l'"
            ],
            "category": "particle"
        }
    },
    "TOKEN_SUFFIX": {
        "de": {
            "pattern": "(jun\\.|sen\\.)",
