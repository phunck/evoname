            "examples": [
                "et",
                "&"
            ],
            "category": "conjunction"
        }
    },
    "TOKEN_SUFFIX": {
        "de": {
            "pattern": "\\b(jun|sen|Jr|Sr|VIII|VII|III|VI|IV|IX|II|V|X)(\\.|\\b)",
            "flags": "i",
            "description": "Suffixe (inkl. Jr/Sr für Mixed Data)",
            "examples": [
